{
    "name": "easeappphp/highper-blueprint",
    "description": "High-performance microservice application built with EaseAppPHP Highper framework",
    "type": "project",
    "license": "MIT",
    "keywords": ["microservice", "api", "async", "high-performance", "c10m"],
    "authors": [
        {
            "name": "EaseAppPHP Team"
        }
    ],
    "require": {
        "php": "^8.2",
        "easeappphp/highper": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "phpbench/phpbench": "^1.2",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "EaseAppPHP\\HighPerBlueprint\\App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EaseAppPHP\\HighPerBlueprint\\App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php public/index.php",
        "test": "phpunit",
        "analyze": "phpstan analyze",
        "cs": "phpcs",
        "benchmark": "php bin/console benchmark"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
