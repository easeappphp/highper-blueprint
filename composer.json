{
    "name": "easeappphp/highper-blueprint",
    "description": "High-performance microservice application built with EaseAppPHP Highper framework",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "easeappphp/highper": "^1.0"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.2",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "EaseAppPHP Team"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}